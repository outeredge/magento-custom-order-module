{
    "name": "outeredge/magento-custom-order-module",
    "type": "magento-module",
    "description": "Create Custom Order for Magento 1.9",
    "license": "MIT",
    "authors": [
        {
            "name": "outer/edge",
            "email": "support@outeredgeuk.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "extra": {
        "map": [
            ["app/code/local/Edge/CustomOrder", "app/code/local/Edge/CustomOrder"],
            ["app/etc/modules/Edge_CustomOrder.xml", "app/etc/modules/Edge_CustomOrder.xml"],
            ["app/design/adminhtml/default/default/layout/edge/customorder.xml", "app/design/adminhtml/default/default/layout/edge/customorder.xml"],
            ["app/design/frontend/base/default/layout/edge/customorder.xml", "app/design/frontend/base/default/layout/edge/customorder.xml"],
            ["app/design/frontend/base/default/template/edge/email/quote/items.phtml", "app/design/frontend/base/default/template/edge/email/quote/items.phtml"],
            ["app/locale/en_GB/template/email/edge_custom_order.html", "app/locale/en_GB/template/email/edge_custom_order.html"],
            ["js/edge/customorder/customorder.js", "js/edge/customorder/customorder.js"]
        ]
    }
}
